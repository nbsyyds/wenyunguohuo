.init()\n  },\n  methods: {\n    init: function() {\n      listExpress().then(response => {\n        this.dataForm = response.data.data\n      })\n    },\n    cancel() {\n      this.init()\n    },\n    update() {\n      this.$refs['dataForm'].validate((valid) => {\n        if (!valid) {\n          return false\n        }\n        this.doUpdate()\n      })\n    },\n    doUpdate() {\n      updateExpress(this.dataForm).then(response => {\n        this.$notify.success({\n          title: '成功',\n          message: '运费配置修改成功'\n        })\n      }).catch(response => {\n        this.$notify.error({\n          title: '失败',\n          message: response.data.errmsg\n        })\n      })\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/config"}]}